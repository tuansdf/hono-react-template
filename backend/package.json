{
  "name": "hono-template",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "start": "bun run dist/server.js",
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "better-auth": "^1.4.18",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.9",
    "hono-rate-limiter": "^0.5.3",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.2",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
